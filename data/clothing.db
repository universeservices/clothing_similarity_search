import sqlite3

conn = sqlite3.connect('data/clothing.db')

c = conn.cursor()
c.execute('''CREATE TABLE clothing_items
             (id INTEGER PRIMARY KEY AUTOINCREMENT,
              description TEXT,
              url TEXT)''')

items = [
    ('1', 'Blue T-shirt', 'https://example.com/blue-tshirt'),
    ('2', 'Black Dress', 'https://example.com/black-dress'),
    # Add more clothing items as needed
]

c.executemany('INSERT INTO clothing_items VALUES (NULL, ?, ?)', items)
conn.commit()

c.execute('SELECT * FROM clothing_items')
items = c.fetchall()

for item in items:
    print(item)
